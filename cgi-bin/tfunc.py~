#-*-coding:utf8-*-
import re,textwrap
def getPartNum (line,partNum) :
	line=line.strip()
	if len(line) != 0 and line[0] is "[" :
		z=line.split("]")[0][1:]
		if not re.search("[^๐-๙0-9]", z) :
			return z
		else :
			return partNum	  
	else :
		return partNum
		
def getTitle(line,title) :
	if  line.find("<h")>-1 :
		return re.sub("<.*?>", "",line)
	else :
		return title

def specialChar(line):
	return line.replace("ฃ","ญ").replace("ฅ","ฐ")
		
def wrapPali(p, leng=60):
	lresult=[]
	loop=True
	while loop:
		cur=p[0:leng]
		cur4=cur[-4:]
		p=p[leng:]
		if len(p) < leng :
			cur,p=swapWrapChar (cur,p)
			lresult.append(cur)
			lresult.append(p)
			break
		p4=p[:4]
		if cur4[-1]==" " or p4[0] == " ":
			lresult.append(cur)
			continue
		else:
			cur,p=swapWrapChar (cur,p)
		lresult.append(cur)
			
	return lresult
	
def swapWrapChar (cur, next):
	p=next
	if p!="" and cur !="":
		cur4=cur[-4:]
		p4=p[:4]
		if " " in cur4:
			z=cur.rsplit(" ", 1)
			cur=z[0]
			p=z[1]+p
		elif " " in p4:
			z=p.split(" ", 1)
			p=z[1]
			cur+=z[0]
		elif "<" in cur4:
			z=cur.rsplit("<",1)
			cur=z[0]
			p="<"+z[1]+p	
		elif ">" in p4:
			z=p.split(">", 1)
			p=z[1]
			cur=cur+z[0]+">" 
		elif re.search("โ|เ|\(".decode("utf8").encode("tis620"), cur[-1]) :
			p=cur[-1]+p
			cur=cur[:-1]+"-"
		elif re.search("า|ิ|ี|ุ|ู|ฯ|\.|\)|ฺ|ํ".decode("utf8").encode("tis620"), p[0]):
			cur+=p[0]+"-"
			p=p[1:]
		else :
			cur+="-"
	return [cur,p]

